c = "`heTfl g as iicpCTo{7F4NRP051N5_16_35P3X51N3_V091B0AE}2"
cList = list(c)
nList = []
nc = ""
for i, char in enumerate(cList):
    if ((i + 1) % 3 == 0):
        nList.append(cList[i])
        nList.append(cList[i-2])
        nList.append(cList[i-1])
        
for char in nList:
    nc += char

print(nc)
